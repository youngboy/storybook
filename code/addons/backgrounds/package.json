      "./src/manager.tsx",
      "./src/preview.tsx"
    ]
  },
  "gitHead": "df30e7db2b251418af106345e5722477f057ec36",
  "storybook": {
    "displayName": "Backgrounds",
    "icon": "https://user-images.githubusercontent.com/263385/101991667-479cc600-3c7c-11eb-96d3-410e936252e7.png",
