  ],
  "scripts": {
    "check": "tsc --noEmit",
    "prepare": "../../../scripts/prepare/bundle.ts"
  },
  "dependencies": {
    "@storybook/addons": "7.0.0-alpha.16",
    "@storybook/api": "7.0.0-alpha.16",
    "@storybook/client-logger": "7.0.0-alpha.16",
    "@storybook/components": "7.0.0-alpha.16",
    "@storybook/core-events": "7.0.0-alpha.16",
    "@storybook/csf": "0.0.2--canary.0899bb7.0",
    "@storybook/theming": "7.0.0-alpha.16",
    "core-js": "^3.8.2",
    "global": "^4.4.0",
    "memoizerific": "^1.11.3",
    "ts-dedent": "^2.0.0",
    "util-deprecate": "^1.0.2"
  },
  "devDependencies": {
    "typescript": "~4.6.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
